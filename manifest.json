{
    "manifest_version" :  3,
    "name" : "point",
    "description" :"extensions by Quema",
    "version" : "0.1",
    "permissions" : [
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "console.js"
    },
    "content_scripts" : [
        {
            "matches" : ["https://www.twitch.tv/*"],
            "js" : ["point.js"]
        }
    ]
}
